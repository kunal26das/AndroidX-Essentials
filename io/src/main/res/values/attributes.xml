<?xml version="1.0" encoding="utf-8"?>

<resources>
    <attr name="editable" format="boolean" />
    <attr name="mandatory" format="boolean" />
    <attr name="regex" format="string" />
    <declare-styleable name="AutoComplete">
        <attr name="array" format="reference" />
        <attr name="android:imeOptions" />
        <attr name="android:lines" />
        <attr name="android:maxLines" />
        <attr name="android:minLines" />
        <attr name="editable" />
        <attr name="mandatory" />
    </declare-styleable>
    <declare-styleable name="TextInput">
        <attr name="android:imeOptions" />
        <attr name="android:inputType" />
        <attr name="android:lines" />
        <attr name="android:maxLines" />
        <attr name="android:minLines" />
        <attr name="editable" />
        <attr name="mandatory" />
        <attr name="regex" />
    </declare-styleable>
    <declare-styleable name="Date">
        <attr name="future" format="boolean" />
        <attr name="past" format="boolean" />
    </declare-styleable>
</resources>