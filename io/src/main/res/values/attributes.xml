<?xml version="1.0" encoding="utf-8"?>

<resources>

    <attr name="regex" format="string" />
    <attr name="array" format="reference" />
    <attr name="editable" format="boolean" />
    <attr name="mandatory" format="boolean" />
    <attr name="regexMessage" format="string" />
    <attr name="calendarStyle" format="reference" />
    <attr name="mandatoryMessage" format="string" />

    <declare-styleable name="AutoComplete">
        <attr name="showSoftInputOnFocus" format="boolean" />
        <attr name="listItem" format="reference" />
        <attr name="mandatoryMessage" />
        <attr name="android:filter" />
        <attr name="mandatory" />
        <attr name="editable" />
        <attr name="array" />
    </declare-styleable>

    <declare-styleable name="ChipGroup">
        <attr name="android:checkable" format="boolean" />
        <attr name="chip" format="reference" />
    </declare-styleable>

    <declare-styleable name="Date">
        <attr name="future" format="boolean" />
        <attr name="past" format="boolean" />
        <attr name="android:format" />
        <attr name="calendarStyle" />
    </declare-styleable>

    <declare-styleable name="TextInput">
        <attr name="mandatoryMessage" />
        <attr name="regexMessage" />
        <attr name="mandatory" />
        <attr name="editable" />
        <attr name="regex" />
    </declare-styleable>

    <declare-styleable name="Time">
        <attr name="android:format" />
        <attr name="calendarStyle" />
    </declare-styleable>

</resources>