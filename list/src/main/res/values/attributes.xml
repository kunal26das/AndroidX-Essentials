<?xml version="1.0" encoding="utf-8"?>

<resources>
    <attr name="dividers" format="boolean" />
    <attr name="emptyState" format="reference" />
    <attr name="loadingState" format="reference" />
    <attr name="spanCount" format="integer" />
    <declare-styleable name="List">
        <attr name="android:orientation" />
        <attr name="dividers" />
        <attr name="emptyState" />
        <attr name="loadingState" />
        <attr name="spanCount" />
    </declare-styleable>
    <declare-styleable name="PagedList">
        <attr name="android:orientation" />
        <attr name="dividers" />
        <attr name="emptyState" />
        <attr name="loadingState" />
        <attr name="spanCount" />
    </declare-styleable>
</resources>